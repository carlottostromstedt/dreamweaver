%div.picture.mt-5
  %img#test{alt: "dalle pic", src: "#{@dalle_url}", style: "width: 700px; height: 700px"}/
  .p.my-4#story-text
    = split_story[0]
  %button.btn.btn-primary.dream-buttons#next-button Next Frame
  .debug-container
    - split_story.each do |i|
      .p.line-text
        = i
    - dalle_urls.each do |i|
      .p.picture-links
        = i

